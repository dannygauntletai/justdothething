{
  "version": 2,
  "buildCommand": "yarn install && yarn build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_NODE_ENV": "production",
    "VITE_API_URL_PROD": "https://justdothething.onrender.com/api",
    "VITE_FRONTEND_URL_PROD": "https://justdothething-website.vercel.app"
  }
} 